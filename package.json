  },
  "repository": {
    "type": "git",
    "url": "YOUR_PROJECT_URL"
  },
  "license": "MIT",
  "dependencies": {
    "@types/benchmark": "^1.0.31",
    "@types/node": "^10.11.3",
    "@types/ora": "^1.3.4",
    "@types/table": "^4.0.5",
    "benchmark": "^2.1.4",
    "markdown-table": "^1.1.2",
    "ora": "^3.0.0",
    "table": "^5.0.2"
  },
  "devDependencies": {
    "eslint-plugin-prettier": "^2.7.0",
    "github-semantic-version": "^7.6.0",
    "husky": "^1.0.0-rc.13",
    "jest": "^23.4.2",
    "lint-staged": "^7.2.0",
    "prettier": "^1.14.2",
    "tslint": "^5.11.0",
